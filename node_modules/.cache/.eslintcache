esktop/NYU/Fifth Year/Build Software/projects/final_project/WeatherApp/src/pages/Login.js",["153","154","155","156"],[],"import React, { useState } from \"react\";\r\nimport '../login.css';\r\nimport { useNavigate } from 'react-router-dom';\r\nimport { useSignup } from '../hooks/useSignup';\r\nimport { useLogin } from '../hooks/useLogin';\r\n\r\nexport default function (props) {\r\n    let authMode = \"signin\";\r\n    let navigate = useNavigate();\r\n\r\n    const changeToSignUpMode = () => {\r\n      localStorage.setItem(\"authMode\", JSON.stringify(\"signup\"));\r\n      window.location.reload();\r\n    }\r\n    \r\n    const changeToLoginMode = () => {\r\n      localStorage.setItem(\"authMode\", JSON.stringify(\"signin\"));\r\n      window.location.reload();\r\n    }\r\n\r\n    let getAuth = JSON.parse(localStorage.getItem(\"authMode\"));\r\n    if (!getAuth){\r\n      localStorage.setItem(\"authMode\", JSON.stringify(authMode));\r\n    }\r\n    else{\r\n      authMode = getAuth;\r\n    }\r\n\r\n    const [loginemail, setloginemail] = useState(\"\");\r\n    const [loginpass, setloginpass] = useState (\"\");\r\n    const [signupemail, setsignupemail] = useState (\"\");\r\n    const [signuppass, setsignuppass] = useState (\"\");\r\n    const {signup, error, isLoading} = useSignup();\r\n    const {login, error: loginError, isLoading: loginIsLoading} = useLogin();\r\n\r\n    const handleLoginEmailChange = (event) => {\r\n      setloginemail(event.currentTarget.value);\r\n    };\r\n\r\n    const handleLoginPassChange = (event) => {\r\n      setloginpass(event.currentTarget.value);\r\n    };\r\n\r\n    const handleSignupEmailChange = (event) => {\r\n      setsignupemail(event.currentTarget.value);\r\n    };\r\n\r\n    const handleSignupPassChange = (event) => {\r\n      setsignuppass(event.currentTarget.value);\r\n    };\r\n\r\n    const handleLogin =  async (e) => {\r\n\r\n      e.preventDefault();\r\n\r\n      await login(loginemail, loginpass);\r\n      \r\n    }\r\n\r\n    const handleSignup = async (e) => {\r\n\r\n      e.preventDefault();\r\n\r\n      await signup(signupemail, signuppass);\r\n\r\n    }\r\n  \r\n    if (authMode === \"signin\") {\r\n      return (\r\n        <div className=\"Auth-form-container\">\r\n          <form className=\"Auth-form\" onSubmit={handleLogin}>\r\n            <div className=\"Auth-form-content\">\r\n              <h3 className=\"Auth-form-title\">Log In</h3>\r\n              <div className=\"text-center\">\r\n                Not registered yet?{\" \"}\r\n                <span className=\"link-primary\">\r\n                <a href=\"#\" onClick={changeToSignUpMode}>Sign Up</a>\r\n                </span>\r\n              </div>\r\n              <div className=\"form-group mt-3\">\r\n                <label>Email</label>\r\n                <input\r\n                  type=\"email\"\r\n                  className=\"form-control mt-1\"\r\n                  placeholder=\"Enter Email\"\r\n                  id=\"loginemail\"\r\n                  onChange={handleLoginEmailChange}\r\n                />\r\n              </div>\r\n              <div className=\"form-group mt-3\">\r\n                <label>Password</label>\r\n                <input\r\n                  type=\"password\"\r\n                  className=\"form-control mt-1\"\r\n                  placeholder=\"Enter password\"\r\n                  id=\"loginpass\"\r\n                  onChange={handleLoginPassChange}\r\n                />\r\n              </div>\r\n              <div className=\"d-grid gap-2 mt-3\">\r\n                <button disabled={loginIsLoading}>Log In</button>\r\n                {loginError && <div className=\"error \">{loginError}</div>}\r\n              </div>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      )\r\n    }\r\n  \r\n    return (\r\n      <div className=\"Auth-form-container\">\r\n        <form className=\"Auth-form\" onSubmit={handleSignup}>\r\n          <div className=\"Auth-form-content\">\r\n            <h3 className=\"Auth-form-title\">Sign Up</h3>\r\n            <div className=\"text-center\">\r\n              Already registered?{\" \"}\r\n              <span className=\"link-primary\" onClick={changeToLoginMode}>\r\n              <a href=\"#\">Log In</a>\r\n              </span>\r\n            </div>\r\n            <div className=\"form-group mt-3\">\r\n              <label>Email</label>\r\n              <input\r\n                type=\"email\"\r\n                className=\"form-control mt-1\"\r\n                placeholder=\"Email\"\r\n                name=\"data\"\r\n                id=\"signupemail\"\r\n                onChange={handleSignupEmailChange}\r\n              />\r\n            </div>\r\n            <div className=\"form-group mt-3\">\r\n              <label>Password</label>\r\n              <input\r\n                type=\"password\"\r\n                className=\"form-control mt-1\"\r\n                placeholder=\"Password\"\r\n                name=\"data\"\r\n                id=\"signuppass\"\r\n                onChange={handleSignupPassChange}\r\n              />\r\n            </div>\r\n            <div className=\"d-grid gap-2 mt-3\">\r\n                <button disabled={isLoading}>Sign up</button>\r\n                {error && <div className=\"error \">{error}</div>}\r\n            </div>\r\n          </div>\r\n        </form> \r\n      </div>\r\n    )\r\n  }",[],"/Users/emmanuelsotoruiz/Desktop/NYU/Fifth Year/Build Software/projects/final_project/WeatherApp/src/pages/AccountSettings.js",[],[],"/Users/emmanuelsotoruiz/Desktop/NYU/Fifth Year/Build Software/projects/final_project/WeatherApp/src/pages/Components/Home/HomeLeft.js",[],[],"/Users/emmanuelsotoruiz/Desktop/NYU/Fifth Year/Build Software/projects/final_project/WeatherApp/src/pages/Components/Home/HomeRight.js",["157"],[],"/Users/emmanuelsotoruiz/Desktop/NYU/Fifth Year/Build Software/projects/final_project/WeatherApp/src/pages/Components/AccountSettings/LeftPa